@{
    ViewData["Title"] = "Home Page";
}
@model User

<h1 class="display-4 mb-5">Enter your information</h1>
<form action="process" method="post">
    <div class="form-group mb-3">
        <label for="FirstName">First Name:</label>
        <input asp-for="FirstName" class="form-control">
        <small><span asp-validation-for="FirstName" class="text-danger"></span></small>
    </div>
    <div class="form-group mb-3">
        <label for="LastName">Last Name:</label>
        <input asp-for="LastName" class="form-control">
        <small><span asp-validation-for="LastName" class="text-danger"></span></small>
    </div>
    <div class="form-group mb-3">
        <label for="Age">Age:</label>
        <input asp-for="Age" class="form-control">
        <small><span asp-validation-for="Age" class="text-danger"></span></small>
    </div>
    <div class="form-group mb-3">
        <label for="Email">Email Address:</label>
        <input asp-for="Email" class="form-control">
        <small><span asp-validation-for="Email" class="text-danger"></span></small>
    </div>
    <div class="form-group mb-3">
        <label for="Password">Password:</label>
        <input asp-for="Password" class="form-control">
        <small><span asp-validation-for="Password" class="text-danger"></span></small>
    </div>
    <input class="btn btn-info" type="submit" value="Submit">
</form>


